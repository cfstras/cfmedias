<h3>Media</h3>
<div class="table-responsive height">
	<table class="table table-striped table-condensed table-hover height">
		<thead><tr>
			<th>Title</th><th>Artist</th><th>Album</th><th>Genre</th>
		</tr></thead>
		<tbody class="scroll">
			{{#each item in model}}
				<tr>
					<td>{{item.Title}}</td>
					<td>{{item.Artist}}</td>
					<td>{{item.Album}}</td>
					<td>{{item.Genre}}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>
